<Group labelreference="L2" drawlabel="true">
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">Wernick := module () </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">export </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">	norm2, powLine, onLine, prodScalV, dotProd, onb, onmed, midp1, midp2, ibar1, ibar2, filterLsLc, mpr, perpend,</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">	A, B, C, Ma, Mb, Mc, Oo, Gg, Ha, Hb, Hc, Hh, Ta, Tb, Tc, Ii, Ea, Eb, Ec, Nn, xA, yA, xB, yB, xC, yC, Rw,</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">	Hw, Wtf, Wt, defFig, drawFig, num, A1, A2, B1, B2, </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">	C1, C2, Lvar, perpendE, onlineE, onPerpendBisectorN, trikX, trikY, N1, N2, WtfVars,</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"><Font encoding="UTF-8">	Ea1, Ea2;  # Ea1, Ea2 pour test, pourront \303\252tre supprim\303\251s</Font></Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"> 	</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"># </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">#</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">#</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">local </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">	onbaux, testnull, testSc, testSl, </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">	 Ma1, Ma2, Mb1, Mb2, Mc1, Mc2, O1, O2, G1, G2, </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">	Ha1, Ha2, Hb1, Hb2, Hc1, Hc2, H1, H2, Ta1, Ta2, Tb1, Tb2, Tc1, Tc2, </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">	I1, I2; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"></Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">option package; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"></Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"># euclidean norm to the square of vect(ab) </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">norm2 := proc (xa, ya, xb, yb)  return (xa-xb)^2+(ya-yb)^2 end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"></Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"># polynomial in (x,y) associated to  line (ab)</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">powLine := proc (x, y, xa, ya, xb, yb)  return (x-xa)*(yb-ya)+(y-ya)*(xa-xb) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"></Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"># point (x,y) on line (ab)</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">onLine := proc (x, y, xa, ya, xb, yb)  return (x-xa)*(yb-ya)+(y-ya)*(xa-xb) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"></Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">#dot product </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">prodScalV := proc (xU, yU, xV, yV)  return xU*xV+yU*yV end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">dotProd  := proc (xU, yU, xV, yV)  return xU*xV+yU*yV end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">perpend  := proc(xM,yM,xP,yP,xQ,yQ,xT,yT) return prodScalV(xP-xM,yP-yM,xT-xQ,yT-yQ) end proc;</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"></Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"></Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"># specific equations for Ex</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"># used for external input</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"># perpend(S,2*e(A)-A,B,C),perpendE(XEAo,YEAo,XSo,YSo,XAo,YAo,XBo,YBo,XCo,YCo)</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">perpendE := proc(XEA,YEA,XS,YS,XA,YA,XB,YB,XC,YC) </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">		return perpend(XS, YS, 2*XEA - XA, 2*YEA - YA, XB, YB, XC, YC)</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">	    end proc;</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"></Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"># collinear(2*e(A)-A,h(B),B),onlineE(XEAo,YEAo, XHBo, YHBo, XAo, YAo, XBo, YBo)</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"># caution!!!! XEA, YEA, XHB and YHB should be numerical values</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">onlineE := proc(XEA,YEA, XHB, YHB, XA, YA, XB, YB)</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">		onLine(2*XEA-XA, 2*YEA-YA, XHB, YHB, XB, YB)</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">	   end proc;</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"></Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"># sepcific equation for N</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"># onPerpendBisector(2*n,A+B,2*h(P))</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">onPerpendBisectorN := proc(xN,yN,XHP,YHP,XA,YA,XB,YB)</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			onmed(2*xN,2*yN, XA + XB, YA + YB, 2*XHP, 2*YHP)</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">		      end proc;</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"></Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">#specific equations for Ex</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"># caution!!! xO, yO, XEA, and YEA should have numeric values</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"># egx(midpoint(B+C,2*A),midpoint(2*e(A),2*o)), trikX(XEAo,YEAo,xO,yO, XAo, YAo, XBo, YBo, XCo, YCo)</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">trikX := proc(XEA,YEA,xO,yO, XA, YA, XB, YB, XC, YC)</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">		 XB+XC + 2*XA - (2*XEA + 2*xO)</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">        end proc;</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"></Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">trikY := proc(XEA,YEA,xO,yO, XA, YA, XB, YB, XC, YC)</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">		 YB+YC + 2*YA - (2*YEA + 2*yO)</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">        end proc;</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"></Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"></Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">#on bisectors (including degenerate case): auxiliary function</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">onbaux := proc (x, y, xS, yS, xM, yM, xN, yN) </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			return powLine(x, y, xS, yS, xM, yM)^2*norm2(xS, yS, xN, yN)</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">					-powLine(x, y, xS, yS, xN, yN)^2*norm2(xS, yS, xM, yM) </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">		end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">		</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">#on bisectors</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">onb := proc (x, y, xS, yS, xM, yM, xN, yN) </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">		local q; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">		divide(onbaux(x, y, xS, yS, xM, yM, xN, yN), xM*yN-xM*yS-xN*yM+xN*yS+xS*yM-xS*yN, q); </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">		return q</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">	  end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"></Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">#on perpendicular bisector</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">onmed := proc (x, y, xM, yM, xN, yN) </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">		   return norm2(x, y, xM, yM)-norm2(x, y, xN, yN) </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">		end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"># midpoint</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">midp1 := proc (x, y, xM, yM, xN, yN)  return 2*x-xM-xN end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">midp2 := proc (x, y, xM, yM, xN, yN)  return 2*y-yM-yN end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"></Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"># isobarycenter (center of gravity)</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">ibar1 := proc (x, y, xM, yM, xN, yN, xP, yP)  return 3*x-xM-xN-xP end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">ibar2 := proc (x, y, xM, yM, xN, yN, xP, yP)  return 3*y-yM-yN-yP end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"></Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"># conversion r=0 to true</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">testnull := proc (r) return `if`(r = 0, true, false) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"></Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">#</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"># filtering degenerate systems</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"># filter1 : check if all polynomials in SCond belongs to the ideal &lt;Sys&gt;</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">testSc := proc (Sys, SCond, R) </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			local p, c, test, r;</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			test := true;</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			for c in SCond while test do</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">				r := RegularChains[SparsePseudoRemainder](c, Sys, R); </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">				test := test and testnull(r) </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			end do; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			return test </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">		end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">	 </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"># filter2: check if one set of conditions in LScond belongs to &lt;Sys&gt;</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">testSl := proc (Sys, LScond, R) </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			local SCond, found; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			found := false; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			for SCond in LScond while not found do </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">				found := testSc(Sys, SCond, R) </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			end do; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			return found </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">		end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">		</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"># filter3: apply testSI to the list of systems LSys</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">filterLsLc := proc (LSys, LScond, R)</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			local Lr, Sys; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			Lr := [];</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			for Sys in LSys do </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			   if not testSl(Sys, LScond, R) then </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">				  Lr := [op(Lr), Sys] </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			   end if </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			 end do; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			 return Lr </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">		   end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">#	</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"># printing of a system</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">#</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">mpr := proc (s)</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">	   local p; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">	   for p in s do </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">		   printf(&quot; ---------------------- &quot;);</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">		   print(p) </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">		end do </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">	  end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">	  </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"># </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"># hard coded equations for each coordinate</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"># of the Wernick's characteristic points of a triangle</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">#</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"># point A</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">A1 := proc (x, y)  return xA-x end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">A2 := proc (x, y)  return yA-y end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"></Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">#point B</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">B1 := proc (x, y)  return xB-x end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">B2 := proc (x, y)  return yB-y end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"></Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">#point C</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">C1 := proc (x, y)  return xC-x end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">C2 := proc (x, y)  return yC-y end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"></Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">#midpoint of [BC]</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">Ma1 := proc (xMa, yMa)  return midp1(xMa, yMa, xB, yB, xC, yC) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">Ma2 := proc (xMa, yMa)  return midp2(xMa, yMa, xB, yB, xC, yC) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"></Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">#midpoint of [AC]</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">Mb1 := proc (xMb, yMb)  return midp1(xMb, yMb, xA, yA, xC, yC) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">Mb2 := proc (xMb, yMb)  return midp2(xMb, yMb, xA, yA, xC, yC) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"></Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">#midpoint of [AB]</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">Mc1 := proc (xMc, yMc)  return midp1(xMc, yMc, xA, yA, xB, yB) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">Mc2 := proc (xMc, yMc)  return midp2(xMc, yMc, xA, yA, xB, yB) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"></Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"># circumcenter</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">O1 := proc (xO, yO)  return onmed(xO, yO, xA, yA, xB, yB) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">O2 := proc (xO, yO)  return onmed(xO, yO, xA, yA, xC, yC) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"></Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"># isobarycenter</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">G1 := proc (xG, yG)  return ibar1(xG, yG, xA, yA, xB, yB, xC, yC) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">G2 := proc (xG, yG)  return ibar2(xG, yG, xA, yA, xB, yB, xC, yC) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"></Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"># feet of the altitudes</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"># from point A</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">Ha1 := proc (xHa, yHa)  return prodScalV(xA-xHa, yA-yHa, xC-xB, yC-yB) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">Ha2 := proc (xHa, yHa)  return onLine(xHa, yHa, xB, yB, xC, yC) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"></Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"># from point B</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">Hb1 := proc (xHb, yHb)  return prodScalV(xB-xHb, yB-yHb, xC-xA, yC-yA) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">Hb2 := proc (xHb, yHb)  return onLine(xHb, yHb, xA, yA, xC, yC) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"></Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">#from point C</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">Hc1 := proc (xHc, yHc)  return prodScalV(xC-xHc, yC-yHc, xA-xB, yA-yB) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">Hc2 := proc (xHc, yHc)  return onLine(xHc, yHc, xB, yB, xA, yA) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"></Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"># orthocenter</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">H1 := proc (xH, yH)  return prodScalV(xA-xH, yA-yH, xB-xC, yB-yC) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">H2 := proc (xH, yH)  return prodScalV(xB-xH, yB-yH, xC-xA, yC-yA) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"></Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"># &quot;feet&quot; of the inner bisectors</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"># that is intersection of the inner bisector from one vertex to the opposite side</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"># from point A</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">Ta1 := proc (xTa, yTa)  return onb(xTa, yTa, xA, yA, xB, yB, xC, yC) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">Ta2 := proc (xTa, yTa)  return onLine(xTa, yTa, xB, yB, xC, yC) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"></Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"># from point B</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">Tb1 := proc (xTb, yTb)  return onb(xTb, yTb, xB, yB, xA, yA, xC, yC) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">Tb2 := proc (xTb, yTb)  return onLine(xTb, yTb, xA, yA, xC, yC) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"></Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"># from point C</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">Tc1 := proc (xTc, yTc)  return onb(xTc, yTc, xC, yC, xA, yA, xB, yB) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">Tc2 := proc (xTc, yTc)  return onLine(xTc, yTc, xA, yA, xB, yB) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"></Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"># center of the inscribed circle</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">I1 := proc (xI, yI)  return onb(xI, yI, xA, yA, xB, yB, xC, yC) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">I2 := proc (xI, yI)  return onb(xI, yI, xB, yB, xA, yA, xC, yC) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"></Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"># center of the Euler circle</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">N1 := proc (xN, yN)  return onmed(2*xN, 2*yN, xA+xB, yA+yB, xA+xC, yA+yC) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">N2 := proc (xN, yN)  return onmed(2*xN, 2*yN, xA+xB, yA+yB, xB+xC, yB+yC) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"></Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"># A supprimer</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">Ea1 := proc(x,y) return prodScalV(xA-(2*x- xA), yA-(2*y- yA), xB-xC, yB-yC) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">Ea2 := proc(x,y) return prodScalV(xB-(2*x- xA), yB-(2*y- yA), xC-xA, yC-yA) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"></Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"># pair of equations for each point</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">A := proc (x, y)  A1(x, y), A2(x, y) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">B := proc (x, y)  B1(x, y), B2(x, y) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">C := proc (x, y)  C1(x, y), C2(x, y) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">Ma := proc (x, y)  Ma1(x, y), Ma2(x, y) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">Mb := proc (x, y)  Mb1(x, y), Mb2(x, y) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">Mc := proc (x, y)  Mc1(x, y), Mc2(x, y) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">Oo := proc (x, y)  O1(x, y), O2(x, y) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">Gg := proc (x, y)  G1(x, y), G2(x, y) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">Ha := proc (x, y)  Ha1(x, y), Ha2(x, y) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">Hb := proc (x, y)  Hb1(x, y), Hb2(x, y) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">Hc := proc (x, y)  Hc1(x, y), Hc2(x, y) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">Hh := proc (x, y)  H1(x, y), H2(x, y) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">Ta := proc (x, y)  Ta1(x, y), Ta2(x, y) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">Tb := proc (x, y)  Tb1(x, y), Tb2(x, y) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">Tc := proc (x, y)  Tc1(x, y), Tc2(x, y) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">Ii := proc (x, y)  I1(x, y), I2(x, y) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">Ea := proc (x, y) return Hh(2*x- xA, 2*y- yA) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">Eb := proc (x, y) return Hh(2*x- xB, 2*y- yB) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">Ec := proc (x, y) return Hh(2*x- xC, 2*y- yC) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">Nn := proc (x, y) return N1(x, y), N2(x, y) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"></Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">Lvar:=[xA, yA, xB, yB, xC, yC];</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"></Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">Rw := RegularChains[PolynomialRing]([xA, yA, xB, yB, xC, yC]); </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">Hw := [[xB-xC, yB-yC], [xA-xB, yA-yB], [xA-xC, yA-yC]]; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"></Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">#</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"># Wernick under triangular form (and also, deep thought about maple)</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">#</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">Wtf := proc (F, R, H) </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">		local tr, tr1, st; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">		tr := RegularChains[Triangularize](F, R); </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">		tr1 := filterLsLc(tr, H, R); </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">		st := map(RegularChains[Equations], tr1, R); </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">		return st </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">	 end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">	 </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">Wt := proc (F) return Wtf(F, Rw, Hw) end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"></Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">WtfVars := proc(F, L, H)</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">	local Rcur;</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">	Rcur :=  RegularChains[PolynomialRing](L);</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">	return Wtf(F, Rcur, H);</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">end proc;</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"></Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"># search for a given variable &quot;vx&quot; in a list (li) of equations vx=something</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">num := proc (li, vx) </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">		local i, v, p; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">		i := 1; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">		v := 0; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">		for p in li while v = 0 do </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			if evalb(op(p)[1] = vx) then v := i end if; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			i := i+1 </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">		end do; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">		return v </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">	  end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"></Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"># get the coordinates of all the characteristic points corresponding to a solution</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"># encoded in the liste li = [xA = ..., yA= ..., xB=.. ]</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"># the order of the varables xa, yA, ... may vary because we use num() procedure</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">defFig := proc (li) </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			local x, y, pA, pB, pC; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			 x := num(li, xA); y := num(li, yA); </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			pA := geometry[point]('PA', op(li[x])[2], op(li[y])[2]); </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			x := num(li, xB); y := num(li, yB); </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			pB := geometry[point]('PB', op(li[x])[2], op(li[y])[2]); </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			x := num(li, xC); </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			y := num(li, yC); </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			pC := geometry[point]('PC', op(li[x])[2], op(li[y])[2]); </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			geometry[triangle](T, [pA, pB, pC]); </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			geometry[altitude](hA, pA, T, nHa); </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			geometry[altitude](hB, pB, T, nHb); </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			geometry[altitude](hC, pC, T, nHc); </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			geometry[bisector](bA, pA, T, Ba); </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			geometry[bisector](bB, pB, T, Bb); </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			geometry[bisector](bC, pC, T, Bc); </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			geometry[median](mA, pA, T, iA); </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			geometry[median](mB, pB, T, iB); </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			geometry[median](mC, pC, T, iC); </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			geometry[circumcircle](CC, T, 'centername' = nO); </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			return [pA, pB, pC, T, hA, hB, hC, bA, bB, bC, mA, mB, mC, CC] </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">		end proc; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">		</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"># drawing a figure 	corresponding to a solution encoded in li</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">drawFig := proc (li) </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			local x, y, pA, pB, pC; </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			 x := num(li, xA); y := num(li, yA); </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			geometry[point](pA, op(li[x])[2], op(li[y])[2]); </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			x := num(li, xB); y := num(li, yB); </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			geometry[point](pB, op(li[x])[2], op(li[y])[2]); </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			x := num(li, xC); y := num(li, yC); </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			geometry[point](pC, op(li[x])[2], op(li[y])[2]); </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			geometry[triangle](T, [pA, pB, pC]); </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			geometry[altitude](hA, pA, T, nHa);</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			geometry[altitude](hB, pB, T, nHb); </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			geometry[altitude](hC, pC, T, nHc); </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			geometry[bisector](bA, pA, T, Ba); </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			geometry[bisector](bB, pB, T, Bb); </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			geometry[bisector](bC, pC, T, Bc);</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			geometry[median](mA, pA, T, iA); </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			geometry[median](mB, pB, T, iB); </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			geometry[median](mC, pC, T, iC); </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			geometry[circumcircle](CC, T, 'centername' = nO); </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			geometry[EulerCircle](EC, T, 'centername' = oM); </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">			geometry[draw]([pA, pB, pC, T(color = black, thickness = 1), hA(color = black), hB(color = black), hC(color = black), bA(color = blue), bB(color = blue), bC(color = blue), mA(color = green), mB(color = green), mC(color = green), CC(color = pink, linestyle = dot, thickness = 1), nO(color = red), EC, oM], axes = normal, printtext = true) </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">		end proc </Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal"></Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">end module;</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">savelib('Wernick');</Text-field>
